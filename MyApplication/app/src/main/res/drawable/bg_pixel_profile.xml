<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/pixel_navy" />
        </shape>
    </item>
    <item android:gravity="center_horizontal|top">
        <shape android:shape="rectangle">
            <gradient
                android:angle="180"
                android:centerColor="#663D8EDB"
                android:endColor="#11242B4F"
                android:startColor="#004C6CB3" />
        </shape>
    </item>
</layer-list>
